{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch --env-file .env src/index.ts",
    "start": "yarn dev",
    "migrate": "yarn prisma generate && yarn prisma migrate dev",
    "studio": "yarn prisma studio",
    "seed": "tsx prisma/seed.ts",
    "build": "tsc -p tsconfig.json && tsc-alias",
    "studio:prod": "dotenv -e .env.prod -- yarn prisma studio",
    "migrate:prod": "dotenv -e .env.prod -- yarn prisma generate && dotenv -e .env.prod -- yarn prisma migrate deploy ",
    "start:prod": "node ./src/index.js",
    "dev:prod": "dotenv -e .env.prod -- tsx watch src/index.ts",
    "lint": "biome lint --write --unsafe"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.817.0",
    "@aws-sdk/lib-storage": "^3.817.0",
    "@biomejs/biome": "1.9.4",
    "@fastify/cors": "^10.0.2",
    "@fastify/swagger": "^9.4.2",
    "@fastify/swagger-ui": "^5.2.1",
    "@prisma/client": "^6.8.2",
    "axios": "^1.7.9",
    "csv-stringify": "^6.5.2",
    "fastify": "^5.2.1",
    "fastify-type-provider-zod": "^4.0.2",
    "zod": "^3.24.1",
    "zod-i18n-map": "^2.27.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/node": "^22.13.1",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "prisma": "^6.8.2",
    "tsc": "^2.0.4",
    "tsc-alias": "^1.8.11",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
